
{
  "expo": {
    "name": "Romantic",
    "slug": "Romantic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/notebook-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/notebook-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E91E63"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.romantic.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/notebook-icon.png",
        "backgroundColor": "#E91E63"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.romantic.app"
    },
    "web": {
      "favicon": "./assets/images/notebook-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notebook-icon.png",
          "color": "#E91E63",
          "defaultChannel": "memories",
          "sounds": []
        }
      ]
    ],
    "scheme": "romantic",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
